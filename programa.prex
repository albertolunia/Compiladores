PROGRAMA PREX {
    FLOAT nota1, nota2, peso1, peso2, soma, divisor, media;

    ESCREVER nota1;
    
	LER nota1;
    ESCREVER peso1;
    LER peso1;
    
    ESCREVER nota2;
    LER nota2;
    ESCREVER peso2;
    LER peso2;
    
    soma = * nota1 peso1;
    divisor = + peso1 peso2;
    soma = + soma * nota2 peso2;
    media = / soma divisor;
    
    ESCREVER media;
}